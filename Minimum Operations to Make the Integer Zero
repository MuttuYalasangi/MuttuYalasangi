class Solution {
    public int makeTheIntegerZero(int num1, int num2) {
        for(int i = 0;i <= 60;i++) {
            long value = (long)num1 - (long)i * num2;
            if(value < 0) break;
            int bits = Long.bitCount(value);
            if(bits <= i && i <= value) {
                return i;
            }
         }
         return -1;
    }
}
