class Solution {
    public int findNumbers(int[] nums) {
        int  evenCount = 0;
        for(int i = 0;i < nums.length;i++) {
            int digitCount = 0;
            int n = nums[i];
            while(n > 0) {
                n = n / 10;
                digitCount++;
            }
            if(di % 2 == 0) {
                evenCount++;

            }
        }
        return evenCount;
        
    }
}
