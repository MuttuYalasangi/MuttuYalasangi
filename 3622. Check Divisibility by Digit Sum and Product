class Solution {
    public boolean checkDivisibility(int n) {
        int original = n;
        int sum = 0;
        int prod = 1;
        while(original > 0) {
            int digit = original % 10;
            sum += digit;
            prod *= digit;
            original /= 10;
        }
        int total = sum + prod;
        if(n % total == 0) return true;
        else return false;
        
    }
}
